# Configuration de la Base de Données
# Choisissez le type de base de données ("supabase" ou "postgresql")
VITE_DB_TYPE=supabase

# Configuration Supabase (si VITE_DB_TYPE=supabase)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configuration PostgreSQL (si VITE_DB_TYPE=postgresql)
VITE_PG_HOST=localhost
VITE_PG_PORT=5432
VITE_PG_DATABASE=inventory
VITE_PG_USER=your_username
VITE_PG_PASSWORD=your_password
VITE_PG_SSL=false
VITE_PG_TIMEZONE=UTC

# Exemple Supabase:
# VITE_SUPABASE_URL=https://xyzcompany.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Exemple PostgreSQL:
# VITE_PG_HOST=localhost
# VITE_PG_PORT=5432
# VITE_PG_DATABASE=inventory
# VITE_PG_USER=postgres
# VITE_PG_PASSWORD=your_secure_password
# VITE_PG_SSL=false
# VITE_PG_TIMEZONE=Europe/Paris